{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": false,
        "sourceMap": false,
        "declaration": false,
        "noEmit": false,
        "outDir": "./"
    },
    "include": [
        "src/sdk/**/*.ts"
    ]
}